{
  "ca-prefix": "/mnemosyne",
  "ca-info": "An example NDNCERT CA for Mnemosyne",
  "max-validity-period": "1296000",
  "max-suffix-length": "2",
  "probe-parameters":
  [
    {"probe-parameter-key": "common-name"},
    {"probe-parameter-key": "site"}
  ],
  "supported-challenges":
  [
    { "challenge": "pin" },
    { "challenge": "email" },
    { "challenge": "possession" }
  ],
  "redirect-to":
  [
    {
      "ca-prefix": "/example/site1",
      "certificate": ""
    }
  ],
  "name-assignment":
  {
     "param": "/site/common-name",
     "random": ""
  }
}
